using System;
using System.Collections.Generic;
using System.Text;

namespace HanaMicron.COMS.Model
{
    public class ElecStatusInfo
    {
		private String lineSeq; // 순서
		private String userOpi; // 결재자 의견
		private String viewFlag; // 열람 여부 Y,N
		private String viewDate; // 열람 시간
		private String decision; // 결재(A),합의(C),통보(N) 여부
		private String status; // 상태 여부
        private String userTitle; // 결재자 직급                
		private String userName; // 결재자 이름
		private String depName; // 결재자 부서명

		public String LineSeq
		{
			get { return lineSeq; }
			set { lineSeq = value; }
		}

		public String UserOpi
		{
			get { return userOpi; }
			set { userOpi = value; }
		}

		public String ViewFlag
		{
			get { return viewFlag; }
			set { viewFlag = value; }
		}

		public String ViewDate
		{
			get { return viewDate; }
			set { viewDate = value; }
		}

		public String Decision
		{
			get { return decision; }
			set { decision = value; }
		}

		public String Status
		{
			get { return status; }
			set { status = value; }
		}

        public String UserTitle
        {
            get { return userTitle; }
            set { userTitle = value; }
        }

		public String UserName
		{
			get { return userName; }
			set { userName = value; }
		}

		public String DepName
		{
			get { return depName; }
			set { depName = value; }
		}
    }
}
