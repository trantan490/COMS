<%@ Page Language="C#" AutoEventWireup="true" CodeFile="viewVisitTime.aspx.cs" Inherits="main_visit_viewTime" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" >
<head runat="server">
    <title>입 출문 시간 상세보기</title>
    <link rel="stylesheet" href="../../include/css/StyleSheet.css" />
</head>
<body>
    <form id="form1" runat="server">
    <div>
		<table style="background-color:#CCCCCC;width:100%" cellpadding="3" cellspacing="1">
			<tr>
				<td class="title" style="background-color:#FFFFFF" colspan="2">
					입출문 시간 상세보기
				</td>
			</tr>
			<tr>
				<td style="background-color:#F5F5F5;width:100px">신청 시간
				</td>
				<td style="background-color:#FFFFFF">
					<asp:Label ID="lblRegDate" runat="server" Text="Label"></asp:Label>
				</td>
			</tr>
			<tr>
				<td style="background-color:#F5F5F5;width:100px">승인 시간
				</td>
				<td style="background-color:#FFFFFF">
					<asp:Label ID="lblApproveTime" runat="server" Text="Label"></asp:Label>
				</td>
			</tr>
			<tr>
				<td style="background-color:#F5F5F5;width:100px">입문 시간
				</td>
				<td style="background-color:#FFFFFF">
					<asp:Label ID="lblINTime" runat="server" Text="Label"></asp:Label>
				</td>
			</tr>
			<tr>
				<td style="background-color:#F5F5F5;width:100px">출문 시간
				</td>
				<td style="background-color:#FFFFFF">
					<asp:Label ID="lblOutTime" runat="server" Text="Label"></asp:Label>
				</td>
			</tr>
			<tr>
				<td colspan="2" align="center" style="background-color:#FFFFFF">
					<img src="../../images/icon/close.gif" alt="닫기" style="cursor:hand" onclick="self.close();" />
				</td>
			</tr>
		</table>
    
    </div>
    </form>
</body>
</html>
